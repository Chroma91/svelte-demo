{
	"name": "animations",
	"version": "0.0.1",
	"scripts": {
		"package": "svelte-kit package",
		"dev:only": "svelte-kit dev",
		"build:only": "svelte-kit build",
		"preview": "svelte-kit preview",
		"tailwind:watch": "NODE_ENV=development postcss src/styles/raw -d src/styles/generated -w",
		"tailwind:build": "TAILWIND_MODE=build NODE_ENV=production postcss src/styles/raw -d src/styles/generated",
		"dev": "concurrently \"npm run dev:only\" \"npm run tailwind:watch\"",
		"build": "npm run tailwind:build && npm run build:only",
		"prepare": "svelte-kit sync",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@playwright/test": "^1.21.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"autoprefixer": "^10.4.7",
		"concurrently": "^7.2.1",
		"eslint": "^8.12.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss-cli": "^9.1.0",
		"postcss-minify": "^1.1.0",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"svelte": "^3.49.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.10.1",
		"tailwindcss": "^3.0.24",
		"tslib": "^2.3.1",
		"typescript": "^4.7"
	},
	"type": "module"
}
